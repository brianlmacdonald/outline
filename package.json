{
 "name": "outline",
 "version": "1.0.0",
 "description": "visual outline project",
 "main": "index.js",
 "scripts": {
  "test": "ava -v --match='!*DB*'",
  "test-db": "ava -v -s --match='DB*'",
  "build-client-watch": "webpack -w",
  "test-watch": "npm run test -- --watch --reporter=min",
  "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
  "start-watch": "nodemon server/start.js --watch server --watch db --watch index.js --watch package.json",
  "start-server": "NODE_ENV='development' nodemon server",
  "lint": "eslint . --ignore-path .gitignore --ext '.js,.jsx'",
  "validate": "check-node-version --node '>= 7.0.0'",
  "setup": "./bin/mkapplink.js",
  "prep": "npm run validate && npm run setup",
  "postinstall": "npm run prep"
 },
