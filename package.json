{
 "name": "outline",
 "version": "1.0.0",
 "description": "visual outline for story writing",
 "main": "index.js",
 "scripts": {
  "test": "cross-env NODE_ENV=test ava -v --match='!*DB*'",
  "dev": "node dev",
  "test-db": "cross-env NODE_ENV=test ava -v -s --match='DB*'",
  "build-client-watch": "webpack -w --config webpack.dev.js",
  "build-client-production": "webpack -p --config webpack.prod.js",
  "test-watch": "npm run test -- --watch --reporter=min",
  "start-dev": "cross-env NODE_ENV=development npm run build-client-watch & npm run start-server",
  "start-watch": "nodemon server/start.js --watch server --watch db --watch index.js --watch package.json",
  "start-server": "cross-env NODE_ENV=development nodemon server",
  "start": "cross-env NODE_ENV=production node server/index.js",
  "lint": "eslint . --ignore-path .gitignore --ext '.js,.jsx'",
  "validate": "check-node-version --node '>= 7.0.0'",
  "setup": "./bin/mkapplink.js",
  "prep": "npm run validate && npm run setup",
  "postinstall": "npm run prep & npm run build-client-production"
 },
 "author": "",
 "license": "ISC",
 "dependencies": {
  "axios": "^0.18.0",
  "babel-plugin-syntax-dynamic-import": "^6.18.0",
  "body-parser": "^1.18.2",
  "chalk": "^2.3.0",
  "check-node-version": "^3.1.1",
  "compression": "^1.7.2",
  "connect-session-sequelize": "^5.2.1",
  "cross-env": "^5.1.4",
  "crypto": "^1.0.1",
  "css-loader": "^0.28.8",
  "d3": "^5.0.0",
  "enzyme": "^3.3.0",
  "express": "^4.16.3",
  "express-session": "^1.15.6",
  "finalhandler": "^1.1.0",
  "history": "^4.7.2",
  "homedir": "^0.6.0",
  "html-webpack-plugin": "^3.2.0",
  "immutability-helper": "^2.7.0",
  "immutable": "4.0.0-rc.9",
  "lodash": "^4.17.5",
  "morgan": "^1.9.0",
  "passport": "^0.4.0",
  "passport-google-oauth": "^1.0.0",
  "pg": "^7.4.0",
  "pretty-error": "^2.1.1",
  "prop-types": "^15.6.1",
  "ramda": "^0.25.0",
  "react": "^16.3.2",
  "react-dnd": "^2.6.0",
  "react-dnd-html5-backend": "^2.6.0",
  "react-dom": "^16.3.2",
  "react-loadable": "^5.4.0",
  "react-redux": "^5.0.6",
  "react-router": "^4.2.0",
  "react-router-dom": "^4.2.2",
  "react-textarea-autosize": "^6.1.0",
  "redux": "^3.7.2",
  "redux-devtools-extension": "^2.13.2",
  "redux-logger": "^3.0.6",
  "redux-mock-store": "^1.5.1",
  "redux-notifications": "^4.0.1",
  "redux-thunk": "^2.2.0",
  "sequelize": "^4.28.6",
  "style-loader": "^0.19.1",
  "webpack": "^4.8.3"
 },
 "devDependencies": {
  "@babel/core": "^7.0.0-beta.42",
  "@babel/polyfill": "^7.0.0-beta.42",
  "@babel/preset-env": "^7.0.0-beta.42",
  "@babel/preset-flow": "^7.0.0-beta.46",
  "@babel/preset-react": "^7.0.0-beta.42",
  "@babel/preset-stage-2": "^7.0.0-beta.42",
  "@babel/preset-stage-3": "^7.0.0-beta.42",
  "@babel/register": "^7.0.0-beta.42",
  "ava": "^1.0.0-beta.3",
  "babel-cli": "^6.26.0",
  "babel-eslint": "^8.2.1",
  "babel-loader": "^8.0.0-beta.2",
  "dateformat": "^3.0.2",
  "enzyme-adapter-react-16": "^1.1.1",
  "eslint": "^4.14.0",
  "eslint-config-standard": "^11.0.0-beta.0",
  "eslint-plugin-ava": "^4.5.1",
  "eslint-plugin-flowtype": "^2.46.3",
  "eslint-plugin-import": "^2.8.0",
  "eslint-plugin-node": "^5.2.1",
  "eslint-plugin-promise": "^3.6.0",
  "eslint-plugin-react": "^7.5.1",
  "eslint-plugin-standard": "^3.0.1",
  "flow-bin": "^0.71.0",
  "nodemon": "^1.14.6",
  "supertest": "^3.0.0",
  "webpack-cli": "^2.0.13",
  "webpack-livereload-plugin": "^1.0.0"
 },
 "ava": {
  "babel": {
   "testOptions": {
    "babelrc": false,
    "presets": [
     "@babel/preset-flow",
     "@babel/preset-react",
     [
      "@babel/preset-stage-2",
      {
       "decoratorsLegacy": true
      }
     ]
    ]
   }
  },
  "require": [
   "@babel/register"
  ]
 }
